force: false
verbose: false
path:
  manifests: ./manifests
  templates: ./templates
  terragrunt: ./terragrunt
strategy:
  name: terragrunt
  description: terragrunt
  template: |
    {{var.env}}/{{ var.account_id }}/
    {{ if eq var.scope "global" }}
      global/{{ var.service }}
    {{ else }}
      regions/{{ var.region }}/{{ var.folder }}/{{ var.service }}
    {{ end }}
