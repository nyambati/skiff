force: false
verbose: false
path:
  manifests: ./testdata/manifests
  templates: ./testdata/templates
  terragrunt: ./testdata/terragrunt
strategy:
  description: Account-based layout with global and regional separation
  template: |
    {{var.env}}/{{ var.account_id }}/
    {{ if eq var.scope "global" }}
      global/{{ var.service }}
    {{ else }}
      regions/{{ var.region }}/{{ var.group }}/{{ var.service }}
    {{ end }}
